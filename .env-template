# Orchestrator environment template
# Copy to .env and replace placeholder values.

# Canonical config (orchestrator-config)
ORCHESTRATOR_CONFIG=/path/to/orchestrator/config.toml

# OpenRouter supervisor
# Required for orchestrator-app runtime.
# OPENROUTER_API_KEY=<replace_with_openrouter_api_key>

# Linear integration
# Required for orchestrator-app runtime and repo scripts.
# LINEAR_API_KEY=<replace_with_linear_api_key>
# Optional script helper override (scripts/linear_graphql.py only).
ORCHESTRATOR_LINEAR_API_URL=https://api.linear.app/graphql

# Shortcut integration
# Required only when ticketing provider is `shortcut`.
# ORCHESTRATOR_SHORTCUT_API_KEY=<replace_with_shortcut_api_key>

# Internal resume token
# Internal resume token; set automatically by orchestrator when restoring Codex sessions.
# ORCHESTRATOR_HARNESS_SESSION_ID=thread-example

# UI golden test helper (developer/test workflow)
ORCHESTRATOR_UPDATE_GOLDENS=0
